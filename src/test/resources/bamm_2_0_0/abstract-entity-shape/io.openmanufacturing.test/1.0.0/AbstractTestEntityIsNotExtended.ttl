@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:2.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AbstractTestEntityIsNotExtended a bamm:Aspect ;
   bamm:name "AbstractTestEntityIsNotExtended" ;
   bamm:properties ( :aspectProperty ) ;
   bamm:operations () .

:aspectProperty a bamm:Property ;
   bamm:name "aspectProperty" ;
   bamm:characteristic :AbstractEntity .

:AbstractEntity a bamm-c:SingleEntity ;
   bamm:name "AbstractEntity" ;
   bamm:dataType :AbstractTestEntity .

:AbstractTestEntity a bamm:AbstractEntity ;
   bamm:name "AbstractTestEntity" ;
   bamm:properties ( :abstractTestProperty ).

:abstractTestProperty a bamm:Property ;
   bamm:name "abstractTestProperty" ;
   bamm:characteristic [
      a bamm:Characteristic ;
      bamm:name "Integer" ;
      bamm:dataType xsd:integer
   ] .
