@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:2.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:ExtendedTestEntity a bamm:Entity ;
   bamm:name "ExtendedTestEntity" ;
   bamm:properties ( :integer ).

:integer a bamm:Property ;
   bamm:name "integer" ;
   bamm:characteristic [
      a bamm:Characteristic ;
      bamm:name "Integer" ;
      bamm:dataType xsd:integer
   ] .

:ExtendingEntity a bamm:Entity ;
   bamm:name "ExtendingEntity" ;
   bamm:extends :ExtendedTestEntity ;
   bamm:properties ( :string ) .

:string a bamm:Property ;
   bamm:name "string" ;
   bamm:characteristic [
      a bamm:Characteristic ;
      bamm:name "String" ;
      bamm:dataType xsd:string
   ] .
