@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:2.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AbstractTestEntityWithOptionalPropertyAndExtendingEntityWithSameProperty a bamm:AbstractEntity ;
   bamm:name "AbstractTestEntityWithOptionalPropertyAndExtendingEntityWithSameProperty" ;
   bamm:properties ( :abstractTestProperty
                     [ bamm:property :abstractOptionalProperty ; bamm:optional true ] ).

:abstractTestProperty a bamm:Property ;
   bamm:name "abstractTestProperty" ;
   bamm:characteristic [
      a bamm:Characteristic ;
      bamm:name "Integer" ;
      bamm:dataType xsd:integer
   ] .

:abstractOptionalProperty a bamm:Property ;
   bamm:name "abstractOptionalProperty" ;
   bamm:characteristic [
      a bamm:Characteristic ;
      bamm:name "String" ;
      bamm:dataType xsd:string
   ] .

:ExtendingEntity a bamm:Entity ;
   bamm:name "ExtendingEntity" ;
   bamm:extends :AbstractTestEntityWithOptionalPropertyAndExtendingEntityWithSameProperty ;
   bamm:properties ( [ bamm:property :abstractOptionalProperty ; bamm:optional true ] ) .
