@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:2.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AbstractTestEntityWithExtendingEntitiesWithSameProperty a bamm:Entity ;
   bamm:name "AbstractTestEntityWithExtendingEntitiesWithSameProperty" ;
   bamm:properties ( :integer ).

:integer a bamm:Property ;
   bamm:name "integer" ;
   bamm:characteristic [
      a bamm:Characteristic ;
      bamm:name "Integer" ;
      bamm:dataType xsd:integer
   ] .

:A a bamm:Entity ;
   bamm:name "A" ;
   bamm:extends :AbstractTestEntityWithExtendingEntitiesWithSameProperty ;
   bamm:properties ( :a ) .

:a a bamm:Property ;
   bamm:name "a" ;
   bamm:characteristic [
      a bamm:Characteristic ;
      bamm:name "PositiveInteger" ;
      bamm:dataType xsd:positiveInteger
   ] .

:B a bamm:Entity ;
   bamm:name "B" ;
   bamm:extends :AbstractTestEntityWithExtendingEntitiesWithSameProperty ;
   bamm:properties ( :a ) .
