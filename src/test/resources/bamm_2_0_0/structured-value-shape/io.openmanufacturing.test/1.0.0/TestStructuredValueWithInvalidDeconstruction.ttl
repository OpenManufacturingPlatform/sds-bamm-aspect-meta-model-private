@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:2.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#> .
@prefix unit: <urn:bamm:io.openmanufacturing:unit:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:TestStructuredValueWithInvalidDeconstruction a bamm-c:StructuredValue ;
   bamm:name "TestStructuredValueWithInvalidDeconstruction" ;
   bamm:dataType xsd:string ;
   bamm-c:deconstructionRule "(asdf)" ;
   bamm-c:elements ( :prop ) .

:prop a bamm:Property ;
   bamm:name "prop" ;
   bamm:exampleValue "prop" ;
   bamm:characteristic bamm-c:Text .
